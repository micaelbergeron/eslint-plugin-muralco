{
  "name": "eslint-plugin-muralco",
  "version": "1.0.0",
  "description": "Standard rules for MURAL",
  "main": "index.js",
  "types": "index.d.ts",
  "repository": {
    "type": "git",
    "url": "https://github.com/muralco/eslint-plugin-muralco.git"
  },
  "author": "MURAL",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/muralco/eslint-plugin-muralco/issues"
  },
  "homepage": "https://github.com/muralco/eslint-plugin-muralco",
  "scripts": {
    "build": "npm run lint && tsc",
    "dist": "rm -rf dist && npm run build && cp package*.json README.md LICENSE dist",
    "lint": "eslint --ext .ts src",
    "pack": "npm run dist && cd dist && npm pack",
    "watch": "tsc -w"
  },
  "keywords": [
    "eslint",
    "eslintplugin"
  ],
  "peerDependencies": {
    "eslint": ">=0.8.0"
  },
  "devDependencies": {
    "@types/eslint": "^6.1.3",
    "@types/node": "^12.12.14",
    "@typescript-eslint/eslint-plugin": "^2.10.0",
    "@typescript-eslint/parser": "^2.10.0",
    "eslint": "^6.7.2",
    "eslint-config-prettier": "^6.7.0",
    "eslint-plugin-prettier": "^3.1.1",
    "prettier": "^1.19.1",
    "typescript": "^3.7.3"
  }
}
